	<Script file="VCB_Options_0.lua"/>
<!-- Casting Bar Preview	-->
	<Frame name="vcbCastbarPreview" frameStrata="BACKGROUND" enableMouse="true" movable="true" dontSavePosition="true" clampedToScreen="true" hidden="true" virtual="true">
		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="0">
				<Texture parentKey="TextBorder" atlas="ui-castingbar-textbox">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="-12"/>
					</Anchors>
				</Texture>
				<Texture parentKey="Background" atlas="ui-castingbar-background">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
				</Texture>
				<Texture parentKey="BorderShield" atlas="ui-castingbar-shield" hidden="false">
					<Size x="29" y="33"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="-27" y="4"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture parentKey="Border" atlas="ui-castingbar-frame">
					<Anchors>
						<Anchor point="TOPLEFT" x="-1" y="2"/>
						<Anchor point="BOTTOMRIGHT" x="1" y="-2"/>
					</Anchors>
				</Texture>
				<FontString parentKey="Text" inherits="SystemFont_Shadow_Small">
					<Size x="0" y="16"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="-8"/>
						<Anchor point="TOPRIGHT" x="0" y="-8"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>
