<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
	<Frame name="vcbOptions0" inherits="vdwTabsPanel" parent="UIParent">
		<Size x="920" y="528"/>
		<Anchors>
			<Anchor point="RIGHT" relativePoint="RIGHT" x="0" y="0"/>
		</Anchors>
		<Frames>
			<Button name="$parentTab1" inherits="vdwTabsButton">
				<Anchors>
					<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="0" y="-24"/>
				</Anchors>
			</Button>
			<Button name="$parentTab2" inherits="vdwTabsButton"/>
			<Button name="$parentTab3" inherits="vdwTabsButton"/>
			<Button name="$parentTab4" inherits="vdwTabsButton"/>
			<Button name="$parentTab5" inherits="vdwTabsButton"/>
