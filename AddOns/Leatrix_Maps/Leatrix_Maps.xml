<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Frame name="LeaMapsGlobalPinTemplate" inherits="BaseHighlightableMapPoiPinTemplate" enableMouseClicks="true" enableMouseMotion="true" mixin="LeaMapsGlobalPinMixin" virtual="true" />
	<Slider name="LeaMapsConfigurationPanelSliderTemplate" orientation="HORIZONTAL" enableMouse="true" virtual="true">
		<HitRectInsets>
			<AbsInset left="0" right="0" top="-10" bottom="-10"/>
		</HitRectInsets>
		<Layers>
			<Layer>
				<Texture parentKey="Left" atlas="Minimal_SliderBar_Left" useAtlasSize="true">
					<Anchors>
						<Anchor point="LEFT"/>
					</Anchors>
				</Texture>
				<Texture parentKey="Right" atlas="Minimal_SliderBar_Right" useAtlasSize="true">
					<Anchors>
						<Anchor point="RIGHT"/>
					</Anchors>
				</Texture>
				<Texture parentKey="Middle" atlas="_Minimal_SliderBar_Middle" useAtlasSize="true">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Left" relativePoint="TOPRIGHT"/>
						<Anchor point="TOPRIGHT" relativeKey="$parent.Right" relativePoint="TOPLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<ThumbTexture parentKey="Thumb" atlas="Minimal_SliderBar_Button" useAtlasSize="false">
			<Size x="16" y="16"/>
		</ThumbTexture>
	</Slider>
</Ui>
