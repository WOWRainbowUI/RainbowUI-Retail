<Bindings>
    <Binding name="AppearanceKnown_TooltipScrollUp" hidden="true">
        AppearanceTooltipTooltip.activeModel:SetFacing(AppearanceTooltipTooltip.activeModel:GetFacing() + 0.3)
    </Binding>
    <Binding name="AppearanceKnown_TooltipScrollDown" hidden="true">
        AppearanceTooltipTooltip.activeModel:SetFacing(AppearanceTooltipTooltip.activeModel:GetFacing() - 0.3)
    </Binding>
</Bindings>