<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/UI.xsd">
  <Frame name="RaidFrameSettings_EditBoxAndButtonTemplate" virtual="true">
    <Size y="50"/>
    <Layers>
      <Layer level="ARTWORK">
        <FontString parentKey="settings_text" inherits="GameFontNormalLeft" justifyH="LEFT">
          <Anchors>
            <Anchor point="LEFT" x="15" y="0"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <EditBox parentKey="editbox" inherits="InputBoxTemplate">
        <Size x="120" y="20"/>
        <Anchors>
          <Anchor point="LEFT" x="200" y="0"/>
        </Anchors>
      </EditBox>
      <Button parentKey="button" inherits="UIPanelButtonTemplate">
        <Size x="80" y="22"/>
        <Anchors>
          <Anchor point="LEFT" relativeKey="$parent.editbox" relativePoint="RIGHT" x="5" y="0"/>
        </Anchors>
      </Button>
		</Frames>
  </Frame>
</Ui>
